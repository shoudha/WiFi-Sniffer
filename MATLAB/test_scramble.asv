no_OFDM_symbols = 6;
Ndatasc = 48*1;
mod_typ = 4;
ncbps = Ndatasc*mod_typ;
ncpc = 4;

k = 0:ncbps - 1;
s = ceil(ncpc/2);

%First de-permutation of de-interleaver
i = s*floor(k/s) + mod(k + floor(16*k/ncbps), s);

%second de-permutation of de-interleaver
j = 16*i - (ncbps - 1)*floor(16*i/ncbps);

x_dei(j + 1,:) = data(k + 1,:);

sum(x_dei(:, 1) ~= data_dei.');


